<stimulus name="orituning" repeats="10" ex="ex_activeFixation" bgColor="128 128 128">
    <params type="gabor" runline="stimLength orientation phase spatial temporal centerx centery radius contrast radius/4">
        <stimLength>20</stimLength>
        <orientation>0:10:170</orientation>
        <phase>0</phase>
        <spatial>.05</spatial>
        <temporal>8</temporal>
        <centerx>-120</centerx>
        <centery>-120</centery>
        <radius>60</radius>
        <contrast>1.0</contrast>
    
        <nStimPerFix>6</nStimPerFix>
        <fixX>0</fixX>
        <fixY>0</fixY>
        <fixRad>5</fixRad>
        <fixColor>[0;0;255]</fixColor>
        <preStimFix>100</preStimFix>
        <interStimInterval>200</interStimInterval>
        <timeToFix>1000</timeToFix>
        <saccadeInitiate>500</saccadeInitiate>
        <saccadeTime>200</saccadeTime>
        <stayOnTarget>150</stayOnTarget>
        <saccadeLength>150</saccadeLength>
        <noFixTimeout>1000</noFixTimeout>
    </params>
    <randoms>
        <saccadeDir>0:359</saccadeDir>
    </randoms>
</stimulus>
