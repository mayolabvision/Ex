<stimulus repeats="5" ex="ex_FreeViewPic" bgColor="128 128 128">
    <conditions type="movie" runline="movieprefix,suffix numframes dwell startframe centerx centery">
        <grouped>
            <movieprefix>[repmat({'v4fv/scenes2/orig_'},1,551),repmat({'v4fv/scenes2/histeq_'},1,551)]</movieprefix>
            <suffix>[1:551, 1:551]</suffix>
        </grouped>
    </conditions>
    <params>
        <conditionFrequency>[ones(1,551), ones(1,551)]</conditionFrequency>
        <numframes>200</numframes>
        <dwell>200</dwell>
        <startframe>1</startframe>
        <centerx>0</centerx>
        <centery>0</centery>
        <fixX>0</fixX>
        <fixY>0</fixY>
        <fixRadX>512</fixRadX>
        <fixRadY>384</fixRadY>
        <fixWait>2000</fixWait>
        <breakFixTimeout>2000</breakFixTimeout>
        <rewardInterval>1000</rewardInterval>
    </params>
</stimulus>
